<mvc:View
	controllerName="antenna.controller.Detail"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
 	<Page 
		id="pageMaster"
	 	title="{i18n>title>}"
		showNavButton="true"
		navButtonPress="onNavBack"
	>	
		<content>
			<Table
				id="protokolls"
				alternateRowColors="false"
				backgroundDesign="Translucent"
				contextualWidth="Inherit"
				fixedLayout="false"
				popinLayout="Block"
				showOverlay="false"
				growing="true"
				growingScrollToLoad="false"
				growingThreshold="5"
				items="{antenna>/Protokollitemsfiltered}"
			>
				<columns>

					<Column
						hAlign="Left">
						<Text text="Antenna Type"/>
					</Column>
					<Column
						hAlign="Left">
						<Text text="ID"/>
					</Column>					
				</columns>
				<items>
					<ColumnListItem
						vAlign="Inherit"
						type="Navigation"
						press="onItemsPress2"
					>
						<cells>

							<ObjectIdentifier
								text="{antenna>AntenneTyp}"
								textDirection="Inherit"
								title=""
								titleActive="false"
								visible="true"
							/>
							<ObjectNumber
								emphasized="true"
								number="{antenna>ProtokolItemID}"
								state="None"
								textAlign="Begin"
								textDirection="Inherit"
								unit=""
							/>							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>